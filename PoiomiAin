Section:NewButton("Better FPS When Hatching", "", function()
	local ScreenGui = Instance.new("ScreenGui")
	local Frame = Instance.new("Frame")
	local TextLabel = Instance.new("TextLabel")
	local TextLabel_2 = Instance.new("TextLabel")

	ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

	Frame.Parent = ScreenGui
	Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Frame.Position = UDim2.new(0.0672727302, 0, 0.185964912, 0)
	Frame.Size = UDim2.new(0, 589, 0, 129)

	TextLabel.Parent = Frame
	TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.Position = UDim2.new(0.0305361822, 0, 0.07751479, 0)
	TextLabel.Size = UDim2.new(0, 552, 0, 50)
	TextLabel.Font = Enum.Font.SourceSans
	TextLabel.Text = "You cannot reverse this are you sure : I recommend only using if you are going to afk grind and sleep"
	TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextLabel.TextSize = 14.000

	TextLabel_2.Parent = TextLabel
	TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel_2.Position = UDim2.new(-0.000260919333, 0, 1.17751479, 0)
	TextLabel_2.Size = UDim2.new(0, 552, 0, 50)
	TextLabel_2.Font = Enum.Font.SourceSans
	TextLabel_2.Text = "There is 10 Seconds Until it FPS Boosts"
	TextLabel_2.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextLabel_2.TextSize = 14.000

	wait(10)
	local SloGui = Instance.new("ScreenGui")
	local Frame2 = Instance.new("Frame")
	local TextLabel = Instance.new("TextLabel")

	SloGui.Name = "SloGui"
	SloGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	SloGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

	Frame2.Parent = SloGui
	Frame2.BackgroundColor3 = Color3.fromRGB(1, 1, 1)
	Frame2.Size = UDim2.new(0, 1920, 0, 1080)

	TextLabel.Parent = Frame
	TextLabel.BackgroundColor3 = Color3.fromRGB(1, 1, 1)
	TextLabel.BorderColor3 = Color3.fromRGB(27, 42, 53)
	TextLabel.BorderSizePixel = 0
	TextLabel.Position = UDim2.new(0.324156284, 0, 0.423391819, 0)
	TextLabel.Size = UDim2.new(0, 396, 0, 130)
	TextLabel.Font = Enum.Font.SourceSans
	TextLabel.Text = "You have disabled your screen for better FPS"
	TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.TextSize = 14.000      
end)

